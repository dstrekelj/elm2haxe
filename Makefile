
js.hello_world :
	cd js/hello_world ; haxe -main HelloWorld -js HelloWorld.js ; open hello_world.html

js.image :
	cd js/image ; haxe -main Image -js Image.js ; open index.html

js.fitted_image :
	cd js/fitted_image ; haxe -main FittedImage -js FittedImage.js ; open index.html

js.cropped_image :
	cd js/cropped_image ; haxe -main CroppedImage -js CroppedImage.js ; open index.html
